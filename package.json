{
  "name": "docstring-generator",
  "displayName": "docstring_generator",
  "description": "A simple code generator to generate docstrings comprised of annotated code on save or on build. See documentation for specific examples and usecases.",
  "version": "0.0.1",
  "files": [
    "extension.ts",
    "GenerateDocstring/**/*"
  ],
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "docstring-generator.helloWorld",
        "title": "Hello World",
        "category": "docstring_generator"
      }, 
      {
        "command": "docstring-generator.testDart",
        "title": "Test Dart",
        "category": "docstring_generator"
      },
      {
        "command": "docstring-generator.testPython",
        "title": "Test Python",
        "category": "docstring_generator"
      },
      {
        "command": "docstring-generator.testJava",
        "title": "Test Java",
        "category": "docstring_generator"
      },
      {
        "command": "docstring-generator.testJavascript",
        "title": "Test Javascript",
        "category": "docstring_generator"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/traverse": "^0.6.32",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@babel/parser": "^7.21.3",
    "@qoretechnologies/python-parser": "^0.4.10",
    "flutter-ast": "^0.0.4-4",
    "java-ast": "^0.3.0",
    "java-parser": "^2.0.4",
    "traverse": "^0.6.7"
  }
}
